							<outline text="&lt;img src=&quot;writers.png&quot; width=&quot;1024&quot; height=&quot;1024&quot;&gt;" isComment="true" created="Tue, 13 Feb 2024 23:24:11 GMT"/>
							<outline text="&lt;/div&gt;"/>
							</outline>
